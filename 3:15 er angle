#include <bits/stdc++.h>
using namespace std;

int main() {
    double H, M;
    cin >> H >> M;
    double hour_angle = 30 * H + 0.5 * M;
    double minute_angle = 6 * M;
    double angle = fabs(hour_angle - minute_angle);
    double result = min(angle, 360 - angle);
    cout << result << endl;
    return 0;
}
