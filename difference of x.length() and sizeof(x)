#include <iostream>
#include <string>
using namespace std;

int main() {
    string x = "Hello World";
    cout << "Length of x: " << x.length() << endl;
    
    return 0;
}
//output: 11 [words number]

#include <iostream>
using namespace std;

int main() {
  int myNumbers[5] = {10, 20, 30, 40, 50};
  cout << sizeof(myNumbers);
  return 0;
}
//output: 20 [bits used]
